{
  "name": "react-native-jw-media-player",
  "version": "0.2.43",
  "description": "React-native Android/iOS plugin for JWPlayer SDK (https://www.jwplayer.com/)",
  "main": "src/index.js",
  "types": "src/types.ts",
  "scripts": {
    "lint": "eslint index.js Example/index.js Example/src"
  },
  "pre-commit": [
    "lint"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chaimPaneth/react-native-jw-media-player.git"
  },
  "keywords": [
    "react",
    "react-native",
    "jwplayer",
    "media",
    "player",
    "mediaplayer",
    "media-player",
    "jw",
    "android",
    "ios",
    "audio",
    "video",
    "sdk"
  ],
  "author": "chaimPaneth <chaim@chaimpaneth.com> (chaimpaneth.com)",
  "license": "MIT",
  "homepage": "https://github.com/chaimPaneth/react-native-jw-media-player#readme",
  "devDependencies": {
    "react": ">= 18.2.0",
    "react-native": ">= 0.72.5",
    "lodash": ">= 4.17.21"
  },
  "files": [
    "src",
    "android",
    "ios",
    "RNJWPlayer.podspec",
    "!android/build",
    "!ios/build",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "common/",
    "lib/",
    "react-native.config.js"
  ],
  "codegenConfig": {
    "name": "rnjwplayer",
    "type": "components",
    "jsSrcsDir": "./src",
    "android": {
      "javaPackageName": "com.appgoalz.rnjwplayer"
    }
  }
}
